
        <footer class="footer container-fluid">
            <div class="center">
                <p>
                    <% getPages('footer').forEach(function(page, index) { %>
                        <a href="<%= config.root %><%= page.path.replace('index.html', '') %>" title="<%= page.title %>"><%= page.title %></a> <% if(getPages('footer').length != index + 1) { %> - <% } %>
                    <% }); %>
                    <br>
                    &copy; <%= date(new Date(), 'YYYY') %> <%= config.author %>. All rights reserved.
                </p>
            </div>
        </footer>
